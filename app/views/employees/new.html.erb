<h1>Create an Employee: </h1>

<%= form_with model: @employee do |f| %>
<%= f.label :first_name %>
<%= f.text_label :first_name %>
<%= f.label :last_name %>
<%= f.text_label :last_name %>
<%= f.label :alias %>
<%= f.text_label :alias %>
<%= f.label :title %>
<%= f.text_field :title %><br>
<%= f.label :office %>
<%= f.text_field :office %><br>
<%= f.label :img_url %>
<%= f.text_field :img_url %>

<%= f.collection_select :dog_id, @dogs, :id, :name, prompt: true %>
<!-- <%= f.collection.select :dog_id, @dogs, :id, :name, prompt: true %> -->
<%= f.submit %>
<% end %>